[*]
end_of_line = lf
insert_final_newline = true

[*.cs]
indent_style = space
indent_size = 4
charset = utf-8-bom

[*.{c,h,cpp}]
indent_style = space
indent_size = 4
charset = utf-8-bom

[*.java]
indent_style = space
indent_size = 4
charset = utf-8-bom

[*.{js,rb,php}]
end_of_line = crlf
indent_style = tab
indent_size = 64
charset = utf-16le

